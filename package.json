{
  "name": "express-notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf public/",
    "web": "npm-run-all -s clean webpack",
    "dev": "npm-run-all -s clean webpack start",
    "start": "node bin/www",
    "webpack": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.js",
    "watch": "onchange \"src/javascripts/**/*.js\" \"src/stylesheets/**/*.scss\" -- npm run web",
    "pm2": "pm2 start bin/www --name=notes"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "fonts.css": "^1.5.0",
    "jquery": "^3.2.1",
    "morgan": "~1.9.0",
    "normalize.css": "^7.0.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "sequelize": "^4.13.5",
    "serve-favicon": "~2.4.5",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "bootstrap": "^3.3.7",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "onchange": "^3.2.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0"
  }
}
